  "stanza:created": "2022-03-29",
  "stanza:updated": "2022-03-29",
  "stanza:parameter": [
    {
      "stanza:key": "data-url",
      "stanza:example": "https://gist.githubusercontent.com/abkunal/98d35b9b235312e90f3e43c9f7b6932b/raw/d5589ddd53731ae8eec7abd091320df91cdcf5cd/miserables.json",
      "stanza:description": "Data source URL",
      "stanza:required": true
    },
    {
      "stanza:key": "data-type",
      "stanza:type": "single-choice",
      "stanza:choice": ["json", "tsv", "csv", "sparql-results-json"],
      "stanza:example": "json",
      "stanza:description": "Data type",
      "stanza:required": true
    },
    {
      "stanza:key": "misc-custom_css_url",
      "stanza:example": "",
      "stanza:description": "Custom CSS URL"
    },
    {
      "stanza:key": "nodes-key",
      "stanza:type": "string",
      "stanza:example": "nodes",
      "stanza:description": "Where to find the nodes array",
      "stanza:required": true
    },
    {
      "stanza:key": "nodes-id_key",
      "stanza:type": "string",
      "stanza:example": "id",
      "stanza:description": "Key of the unique node id"
    },
    {
      "stanza:key": "node-label-key",
      "stanza:type": "string",
      "stanza:example": "id",
      "stanza:description": "Key of the node label"
    },
    {
      "stanza:key": "node-label-margin",
      "stanza:type": "number",
      "stanza:default": 5,
      "stanza:example": 5,
      "stanza:description": "Node labels offset from node center, in px."
    },
    {
      "stanza:key": "node-size-based_on",
      "stanza:type": "single-choice",
      "stanza:choice": ["data key", "fixed", "sum of edges values"],
      "stanza:example": "data key",
      "stanza:description": "What node size is based on"
    },
    {
      "stanza:key": "node-size-key",
      "stanza:type": "text",
      "stanza:example": "group",
      "stanza:description": "Node size key. Valid only if `node-size-based_on` is set to `data key`"
    },
    {
      "stanza:key": "node-size-min",
      "stanza:type": "number",
      "stanza:example": 3,
      "stanza:description": "Minimum node radius in px"
    },
    {
      "stanza:key": "node-size-max",
      "stanza:type": "number",
      "stanza:example": 6,
      "stanza:description": "Maximum node radius in px"
    },
    {
      "stanza:key": "node-size-scale",
      "stanza:type": "single-choice",
      "stanza:choice": ["sqrt", "linear", "log10"],
      "stanza:example": "sqrt",
      "stanza:description": "Scaling function of node size"
    },

    {
      "stanza:key": "nodes-sort-key",
      "stanza:type": "string",
      "stanza:example": "id",
      "stanza:description": "Key of the node label"
    },
    {
      "stanza:key": "node-size-based_on",
      "stanza:type": "single-choice",
      "stanza:choice": ["data key", "sum of edge values"],
      "stanza:example": "sum of edge values",
      "stanza:description": "What node size is based on"
    },
    {
      "stanza:key": "node-size-key",
      "stanza:type": "text",
      "stanza:example": "group",
      "stanza:description": "Node size key. Valid only if `node-size-based_on` is set to `data key`"
    },
    {
      "stanza:key": "node-size-data-key",
      "stanza:type": "string",
      "stanza:example": "",
      "stanza:description": "Set size on the node based on data key"
