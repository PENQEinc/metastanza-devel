      "stanza:key": "node-fixed-size",
      "stanza:type": "number",
      "stanza:example": 3,
      "stanza:description": "Fixed node radius in px"
    },
    {
      "stanza:key": "node-color-key",
      "stanza:type": "text",
      "stanza:example": "group",
      "stanza:description": "Node's color key"
    },
    {
      "stanza:key": "nodes-sort-key",
      "stanza:type": "string",
      "stanza:example": "group",
      "stanza:description": "Sort nodes by this data key value"
